TRISB=%00000001
symbol TUS PORTB.0
SYMBOL LED PORTB.1

BASLA:
IF TUS=0 THEN
IF LED=1 THEN
LOW LED
ELSE
HIGH LED
ENDIF
ENDIF
GOTO BASLA
END
